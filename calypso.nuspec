<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Calypso</id>
    <version>0.1</version>
    <title>calypso</title>
    <description>A module and powershell scripts to deploy custom cultures in environments with limited privileges, such as Azure.</description>
    <tags>powershell customcultures</tags>
    <authors>Geoffrey Smith</authors>
    <projectUrl>https://github.com/geoffreysmith/calypso</projectUrl>
    <licenseUrl>http://opensource.org/licenses/MIT</licenseUrl>
  </metadata>
  <files>
    <file src="calypso.psm1" target="tools" />
    <file src="functions\**\*.*" target="tools\Functions" />
    <file src="libs\*.dll" target="tools\Libs" />
  </files>
</package>
